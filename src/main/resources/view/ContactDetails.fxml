<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="contactDetailPlane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
  <GridPane HBox.hgrow="ALWAYS">
    <VBox fx:id="contactDetailsBox">
      <Label fx:id="name" styleClass="label-header" />
      <Label fx:id="phoneNo" text="Mobile: " />
      <Label fx:id="email" text="Email: " />
      <Label fx:id="address" text="Address: " />
      <VBox fx:id="notesList" />
    </VBox>
  </GridPane>
</HBox>